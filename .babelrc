{
  "plugins": [
    [
      "transform-assets-import-to-string",
      { "baseDir": "static", "baseUri": "https://nextjseslintflowfile.b-cdn.net/" }
    ],
    [
      "module-alias",
      [{ "src": "./src/modules/components", "expose": "components" }]
    ]
  ],
  "env": {
    "development": {
      "presets": "next/babel"
    },
    "production": {
      "presets": "next/babel"
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "next/babel"
      ]
    }
  }
}
