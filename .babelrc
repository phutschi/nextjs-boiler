{
    "plugins": [
        [
            "module-alias",
            [
                { "src" : "./src/modules/components", "expose" : "components" },
            ]
        ]
    ],
    "env": {
        "development": {
            "presets": "next/babel"
        },
        "production": {
            "presets": "next/babel"
        },
        "test": {
            "presets": [
                [
                    "env",
                    {
                        "modules": "commonjs"
                    }
                ],
                "next/babel"
            ]
        }
    }
}
